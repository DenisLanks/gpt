program exemplo

var x int

proc main
    var t1 int

    // x := 10;
    iset x, 10

    // se x >= 20 entao
    ige t1, x, 20
    ifnot t1, senao

    // x := 2;
    iset x, 2

    jmp proximo
    senao:

    // x := 1;
    iset x, 1

    proximo:

    // imprima( x );
    push_sreg
    push_int x
    push_itype
    push_int 1
    libcall imprima
    pop_sreg

    exit 0
endproc

endprogram

