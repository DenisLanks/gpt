program exemplo

var x int

proc main
    var t1 int

    // para x de 1 até 10 faça
    iset x, 1
    teste:
    ile t1, x, 10
    ifnot t1, proximo

    // imprima( x );
    pushiv x
    pushit
    push_1
    lcall io, imprima

    // fim-para
    iinc x, 1
    jmp teste

    proximo:

    exit_0
endproc

endprogram

