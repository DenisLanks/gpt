program exemplo

var x int
var t1 int

proc main
//    var t1 int

    // para x de 1 até 10 faça
    iset x, 1
    teste:
    ile t1, x, 10
    ifnot t1, proximo

    // imprima( x );
    push_sp
    push x
    push_int
    push 1
    pcall imprima
    pop_sp

    // fim-para
    iinc x, 1
    jmp teste

    proximo:

    exit 0
endproc

endprogram

