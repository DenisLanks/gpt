program exemplo

proc main
    var t1 int
    // imprima( "A soma é: ", soma( 2, 7 ) );
    pushiv 0 // resultado da função
    pushiv 2 // empilha 2
    pushiv 7 // empilha 7
    pcall soma
    popiv t1
    pushiv t1
    pushit
    pushsv "A soma é: "
    pushst
    pushiv 2
    libcall imprima

    exit 0
endproc

proc soma
    param __result int
    param x int
    param y int

    isum __result, x, y

    decsp 8 // tamanho dos parametros + variaveis locais - retorno da funcao
    ret
endproc

endprogram

