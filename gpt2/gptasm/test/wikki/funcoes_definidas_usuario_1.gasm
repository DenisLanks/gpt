program exemplo

proc main
    var t1 int
    // imprima( "A soma é: ", soma( 2, 7 ) );
    pushiv 7 // empilha 7
    pushiv 2 // empilha 2
    pcall soma
    popiv t1
    pushiv t1
    pushit
    pushsv "A soma é: "
    pushst
    push_2
    lcall imprima

    exit_0
endproc

proc soma
    param x int
    param y int
    var __result int

    isum __result, x, y

//    decsp 12 // tamanho dos parametros + variaveis locais - retorno da funcao
    iret 12, __result
endproc

endprogram

