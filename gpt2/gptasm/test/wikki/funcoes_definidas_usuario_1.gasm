program exemplo

proc main
    var t1 int
    // imprima( "A soma é: ", soma( 2, 7 ) );
    pushir // resultado da função
    pushiv 7 // empilha 7
    pushiv 2 // empilha 2
    pcall soma
    popiv t1
    pushiv t1
    pushit
    pushsv "A soma é: "
    pushst
    push_2
    lcall imprima

    exit_0
endproc

proc soma
    param __result int
    param x int
    param y int

    isum __result, x, y

    decsp_8 // tamanho dos parametros + variaveis locais - retorno da funcao
    ret
endproc

endprogram

