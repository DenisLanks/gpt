program exemplo

proc main
   push_sreg
   pcall soma
   pop_sreg

   push_sreg
   push_string "Finalizando..."
   push_stype
   push_int 1
   libcall imprima
   pop_sreg

   exit 0
endproc

proc soma
   var temp int

   isum temp, 2, 4

   push_sreg
   push_int temp
   push_itype
   push_int 1
   libcall imprima
   pop_sreg

   ret
endproc
   
endprogram

