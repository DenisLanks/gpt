// Procedimento com passagem de parâmetros por valor
algoritmo exemplo;

variável g: inteiro;

início
    g := 0;
    g := g;
    roda( g, 10 );
fim

procedimento roda( p1: inteiro, p2: inteiro )
variável l1: inteiro;
variável l2: inteiro;
início
    l1 := 0;
    l1 := l2;
    l1 := g;
    g := l1;
    p1 := p2;
    l1 := p1;
    p1 := l1;
    p1 := g;
    g := p1;
    imprima( g, p1, l1 );
fim
